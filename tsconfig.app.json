{
    // "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue",
        "./auto-imports.d.ts",
        "./components.d.ts",
        "./env.d.ts"
    ],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "jsx": "preserve",
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
